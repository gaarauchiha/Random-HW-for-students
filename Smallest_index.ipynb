   "source": [
    "def findSmallestIndex(nums, target):\n",
    "    n = len(nums)\n",
    "    if n <= 1:\n",
    "        return -1\n",
    "    min_after = nums[-1]\n",
    "    result = -1\n",
    "    for i in range(n - 2, -1, -1):\n",
    "        if min_after > target:\n",
    "            result = i\n",
    "        min_after = min(min_after, nums[i])\n",
    "    return result"
   ]
  }
